<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.juwuhe.dao.FilmDao">

    <select id="selectFilm"  resultType="com.juwuhe.entity.Film">
        select * from film
    </select>

    <select id="selectFilmInfo"  resultType="com.juwuhe.entity.Film">
        select * from film where film_id = #{filmid}
    </select>

</mapper>